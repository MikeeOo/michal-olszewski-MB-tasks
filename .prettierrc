{
  "plugins": ["@prettier/plugin-php", "prettier-plugin-tailwindcss"],
  "phpVersion": "8.1",
  "printWidth": 120,
  "tabWidth": 2,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "semi": true,
  "htmlWhitespaceSensitivity": "ignore",
  "arrowParens": "always",
  "endOfLine": "lf"
}